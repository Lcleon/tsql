--Crie uma coluna que contenha a informação de volume em Metros cúbicos:

SELECT 
    product_id,
    product_length_cm *	product_height_cm *	product_width_cm / 1000000 as product_vol_m3,
    product_height_cm,
    product_category_name,
    product_name_lenght,
    product_description_lenght,
    product_photos_qty,
    product_weight_g,
    product_length_cm,
    product_width_cm
FROM
    tb_products;


    
