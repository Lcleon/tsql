SELECT
    product_category_name,
    COUNT(*) as qtde_product,
    MAX(product_weight_g) as mais_pesado,
    MIN(product_weight_g) as mais_leve,
    AVG(product_weight_g) as media_peso
FROM
    tb_products
WHERE
    product_category_name is NOT NULL
AND
    product_category_name != 'alimentos'
AND
    product_category_name <> 'alimentos_bebidas'
AND
    product_category_name <> 'agro_industria_e_comercio'
GROUP BY
    product_category_name;